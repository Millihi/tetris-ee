<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
   <comment>JSR-303 Bean Validation RU messages</comment>
   <entry key = "javax.validation.constraints.Null.message">
      Значение должно быть нулевым.
   </entry>
   <entry key = "javax.validation.constraints.NotNull.message">
      Значение должно быть ненулевым.
   </entry>
   <entry key = "javax.validation.constraints.AssertTrue.message">
      Значение должно быть истиной.
   </entry>
   <entry key = "javax.validation.constraints.AssertFalse.message">
      Значение должно быть ложью.
   </entry>
   <entry key = "javax.validation.constraints.Min.message">
      Значение должно быть больше или равно {value}.
   </entry>
   <entry key = "javax.validation.constraints.Max.message">
      Значение должно быть меньше или равно {value}.
   </entry>
   <entry key = "javax.validation.constraints.Size.message">
      Размер должен быть в пределах от {min} до {max}.
   </entry>
   <entry key = "javax.validation.constraints.Digits.message">
      Значение должно быть числом вида &lt;{integer} цифр&gt;.&lt;{fraction} цифр&gt;.
   </entry>
   <entry key = "javax.validation.constraints.Past.message">
      Значение должно быть датой прошлого.
   </entry>
   <entry key = "javax.validation.constraints.Future.message">
      Значение должно быть датой будущего.
   </entry>
   <entry key = "javax.validation.constraints.Pattern.message">
      Значение должно соответствовать шаблону {regexp}
   </entry>
</properties>
